{
    "version": "0.2.0",
    "model_type": "e5-large",
    "dataset": "ogbn-arxiv",
    "suffix": "main",
    "output_dir": "./out/${dataset}/${model_type}/${suffix}",
    "ckpt_dir": "${output_dir}/ckpt",
    "configurations": [

        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Attach",
            "type": "debugpy",
            "request": "attach",
            "justMyCode": false,
            "connect": {
                "host": "localhost",
                "port": 12346
            }
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        }
    ]
}